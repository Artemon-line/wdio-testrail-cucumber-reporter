{
    "name": "wdio-testrail-cucumber-reporter",
    "version": "0.4.1",
    "description": "A Testrail reporter for webdriver.io using cucumber including TestRail API basic library (fork from wdio-testrail-reporter)",
    "main": "lib/wdio-testrail-cucumber-reporter.js",
    "private": false,
    "keywords": [
        "spec",
        "testrail",
        "reporter",
        "webdriverio",
        "wdio",
        "wdio-plugin",
        "wdio-reporter",
        "cucumber"
    ],
    "author": {
        "name": "Gavin Samuels",
        "email": "gavin.samuels10@gmail.com"
    },
    "license": "MIT",
    "readmeFilename": "README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Artemon-line/wdio-testrail-cucumber-reporter"
    },
    "bugs": {
        "url": "https://github.com/Artemon-line/wdio-testrail-cucumber-reporter/issues"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "file": "^0.2.2",
        "mocha-testrail-reporter": "^1.0.12",
        "sync-request": "^6.1.0",
        "mocha": "^6.1.4"
    },
    "scripts": {
        "lint": "./node_modules/.bin/eslint",
        "test": "./node_modules/.bin/_mocha",
